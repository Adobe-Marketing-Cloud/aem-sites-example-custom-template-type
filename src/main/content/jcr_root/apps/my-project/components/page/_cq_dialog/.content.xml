<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]"
    mode="edit">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <facebook jcr:primaryType="nt:unstructured"
                        jcr:title="Facebook Open Graph"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <url
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        cq-msm-lockable="ogUrl"
                                        fieldLabel="URL"
                                        name="./ogUrl"
                                        renderReadOnly="{Boolean}true"/>
                                    <app-id
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        cq-msm-lockable="ogAppId"
                                        fieldLabel="App Id"
                                        name="./ogAppId"
                                        renderReadOnly="{Boolean}true"/>
                                    <site-name
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        cq-msm-lockable="ogSiteName"
                                        fieldLabel="Site Name"
                                        name="./ogSiteName"
                                        renderReadOnly="{Boolean}true"/>
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        cq-msm-lockable="ogTitle"
                                        fieldLabel="Page Title"
                                        name="./ogTitle"
                                        renderReadOnly="{Boolean}true"/>
                                    <type
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        cq-msm-lockable="ogType"
                                        fieldLabel="Type"
                                        name="./ogType"
                                        renderReadOnly="{Boolean}true"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        cq-msm-lockable="ogDescription"
                                        fieldLabel="Description"
                                        name="./ogDescription"
                                        renderReadOnly="{Boolean}true"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        cq-msm-lockable="ogImage"
                                        fieldLabel="Image path"
                                        name="./ogImage"
                                        renderReadOnly="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </facebook>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
